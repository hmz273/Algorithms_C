#include<stdio.h>
int main(){
double num1
double num2
char op
printf("enter nomber1: \n");
scanf("%lf",&num1);
printf("enter a operator: \n");
scanf("%c",&op);
printf("enter nomber2: \n");
scanf("%d",&num2);
if (op=='+'){
printf("%f",num1+num2);
}
 else if (op=='-'){
printf("%f",num1-num2);
}
else if (op=='*'){
printf("%f",num1*num2);
}
else if (op=='/'){
printf("%f",num1/num2);
}
else("error");
